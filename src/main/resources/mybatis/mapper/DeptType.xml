<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaosama.HisSystem.mapper.DeptTypeMapper">
        <select id="select" resultType="com.xiaosama.HisSystem.entity.DeptType">
            select * from dept_type where id =#{id}
        </select>
        <insert id="insert">
            insert into dept_type value(null,#{value})
        </insert>
</mapper>